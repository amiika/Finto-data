		<skos:inScheme rdf:resource="http://jhsmeta.fi/skos/"/>
		<rdfs:seeAlso rdf:resource="http://jhsmeta.fi/page/kotiosoite"/>
		<status:term_status>Unstable</status:term_status>
	</skos:Concept>
	<skos:Concept rdf:about="http://jhsmeta.fi/skos/J114">
		<skos:broader rdf:resource="http://jhsmeta.fi/skos/J104"/>
		<skos:prefLabel xml:lang="fi">virallinen osoite</skos:prefLabel>
		<skos:definition xml:lang="fi">viranomaisen rekisteröimä julkinen osoite</skos:definition>
