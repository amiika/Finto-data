		<skos:definition xml:lang="fi">kirjain, joka on lisätty kiinteistön osoitenumeroon silloin, kun alkuperäinen kiinteistö on jaettu kahdeksi tai useammaksi erilliseksi kiinteistöksi</skos:definition>
		<skos:broader rdf:resource="http://jhsmeta.fi/skos/J249"/>
		<skos:example xml:lang="fi">b</skos:example>
		<skos:inScheme rdf:resource="http://jhsmeta.fi/skos/"/>
		<rdfs:seeAlso rdf:resource="http://jhsmeta.fi/page/osoitenumeron%20jakokirjain"/>
		<status:term_status>Recommendation</status:term_status>
	</skos:Concept>
	<skos:Concept rdf:about="http://jhsmeta.fi/skos/J425">
		<skos:broader rdf:resource="http://jhsmeta.fi/skos/J249"/>
		<skos:prefLabel xml:lang="fi">osoitenumeron numero-osa</skos:prefLabel>
