	</skos:Concept>
	<skos:Concept rdf:about="http://jhsmeta.fi/skos/J116">
		<skos:broader rdf:resource="http://jhsmeta.fi/skos/J114"/>
		<skos:prefLabel xml:lang="fi">kotiosoite</skos:prefLabel>
		<skos:definition xml:lang="fi">henkilön virallinen osoite</skos:definition>
