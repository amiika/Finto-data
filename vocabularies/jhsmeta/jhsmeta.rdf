	<skos:Concept rdf:about="http://jhsmeta.fi/skos/J141">
		<skos:broader rdf:resource="http://jhsmeta.fi/skos/J104"/>
		<skos:prefLabel xml:lang="fi">postilokero-osoite</skos:prefLabel>
		<skos:definition xml:lang="fi">osoite, joka ilmaisee postin vastaanottajan vuokraamaa tai postinjakelijan osoittamaa postinjättöpaikkaa</skos:definition>
		<skos:broader rdf:resource="http://jhsmeta.fi/skos/J104"/>
		<skos:note xml:lang="fi">Postilokero-osoite koostuu postilokerolyhenteestä ja numerosta.</skos:note>
