	<skos:Concept rdf:about="http://example.org/sos/ns#TALOUSRID95">
		<dcterms:identifier rdf:resource="TALOUSRID95"/>
		<skos:inScheme rdf:resource="http://example.org/sos/ns#ExampleScheme"/>
		<skos:prefLabel xml:lang="fi">työttömyyseläke</skos:prefLabel>
		<skosxl:prefLabel rdf:resource="http://example.org/sos/ns#idtext4x18268"/>
		<ts:term_status>Unstable</ts:term_status>
		<skos:definition xml:lang="fi">[eläke](sos:TALOUSRID86), jota voi saada tietyn iän saavuttanut, tietynlaisen työhistorian omaava ja pitkäaikaisesti työtön henkilö ennen [vanhuuseläkettä (1)](sos:TALOUSRID94)</skos:definition>
		<skos:note xml:lang="fi">Työttömyyseläkettä maksetaan [kansaneläkkeenä](sos:TALOUSRID87).</skos:note>
		<skos:note xml:lang="fi">Työttömyyseläkkeestä säädetään esimerkiksi kansaneläkelain voimaanpanosta annetussa laissa (569/2007) ja kunnallisessa eläkelaissa (549/2003).</skos:note>
		<skos:related rdf:resource="http://example.org/sos/ns#TALOUSRID48"/>
		<skos:related rdf:resource="http://example.org/sos/ns#TALOUSRID87"/>
		<skos:related rdf:resource="http://example.org/sos/ns#TALOUSRID89"/>
