		<skos:related rdf:resource="http://example.org/sos/ns#SOPARID17"/>
		<skos:related rdf:resource="http://example.org/sos/ns#STAKESRID175"/>
		<skos:related rdf:resource="http://example.org/sos/ns#ID46"/>
		<skos:related rdf:resource="http://example.org/sos/ns#ASKIRID5"/>
		<skos:broader rdf:resource="http://example.org/sos/ns#ASKIRID36"/>
	</skos:Concept>
